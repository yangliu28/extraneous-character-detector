{
    "name": "extraneous-character-detector",
    "displayName": "Extraneous Character Detector",
    "publisher": "yangliu28",
    "description": "Detect and highlight suspicious characters",
    "version": "0.0.1",
    "icon": "images/asdf.png",
    "engines": {
        "vscode": "^1.42.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "contributes": {
        "languages": [{
            "id": "asdf",
            "aliases": ["asdf", "asdf"],
            "extensions": [".asdf"],
            "configuration": "./language-configuration.json"
        }],
        "grammars": [{
            "language": "asdf",
            "scopeName": "source.asdf",
            "path": "./syntaxes/asdf.tmLanguage.json"
        }]
    },
	"repository": {
		"type": "git",
		"url": "https://github.com/yangliu28/extraneous-character-detector.git"
	}
}